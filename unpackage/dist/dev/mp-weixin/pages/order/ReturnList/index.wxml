<view data-ref="container" class="return-list vue-ref"><block wx:for="{{orderList}}" wx:for-item="order" wx:for-index="orderListIndex" wx:key="orderListIndex"><view class="goodWrapper"><block wx:if="{{order._status._type===-1}}"><view class="iconfont icon-tuikuanzhong powder"></view></block><block wx:if="{{order._status._type===-2}}"><view class="iconfont icon-yituikuan"></view></block><view class="orderNum">{{"订单号："+order.orderId}}</view><block wx:for="{{order.cartInfo}}" wx:for-item="cart" wx:for-index="cartInfoIndex" wx:key="cartInfoIndex"><view data-event-opts="{{[['tap',[['goOrderDetails',['$0'],[[['orderList','',orderListIndex]]]]]]]}}" class="item acea-row row-between-wrapper" bindtap="__e"><view class="pictrue"><image class="image" src="{{cart.productInfo.image}}" data-event-opts="{{[['tap',[['goGoodsCon',['$0'],[[['orderList','',orderListIndex],['cartInfo','',cartInfoIndex]]]]]]]}}" catchtap="__e"></image></view><view class="text"><view class="acea-row row-between-wrapper"><view class="name line1">{{cart.productInfo.storeName}}</view><view class="num">{{"x "+cart.cartNum}}</view></view><block wx:if="{{cart.productInfo.attrInfo}}"><view class="attr line1">{{cart.productInfo.attrInfo.suk}}</view></block><block wx:else><view class="attr line1">{{cart.productInfo.storeName}}</view></block><view class="money">{{"￥"+cart.productInfo.price}}</view></view></view></block><view class="totalSum">{{'共'+(order.cartInfo.length||0)+'件商品，总金额'}}<text class="font-color-red price">{{"￥"+order.payPrice}}</text></view></view></block><block wx:if="{{orderList.length===0&&page>1}}"><view class="noCart"><view class="pictrue"><image src="{{$VUE_APP_RESOURCES_URL+'/images/noOrder.png'}}"></image></view></view></block><loading vue-id="1" loaded="{{loaded}}" loading="{{loading}}" bind:__l="__l"></loading></view>