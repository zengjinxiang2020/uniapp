<view data-ref="container" class="vue-ref"><block wx:if="{{couponsList.length>0}}"><view class="coupon-list"><block wx:for="{{couponsList}}" wx:for-item="item" wx:for-index="couponsListIndex" wx:key="couponsListIndex"><view class="item acea-row row-center-wrapper"><view class="{{['money',item.isUse?'moneyGray':'']}}">￥<text class="num">{{item.couponPrice}}</text></view><view class="text"><view class="condition line1">{{"购物满"+item.useMinPrice+"元可用"}}</view><view class="data acea-row row-between-wrapper"><block wx:if="{{item.endTime!==0}}"><view><data-format-t vue-id="{{'1-'+couponsListIndex}}" data="{{item.startTime}}" bind:__l="__l"></data-format-t>-<data-format-t vue-id="{{'2-'+couponsListIndex}}" data="{{item.endTime}}" bind:__l="__l"></data-format-t></view></block><block wx:else><view>不限时</view></block><block wx:if="{{item.isUse===true}}"><view class="bnt gray">已领取</view></block><block wx:else><block wx:if="{{item.isUse===2}}"><view class="bnt gray">已领完</view></block><block wx:else><view data-event-opts="{{[['tap',[['getCoupon',['$0',couponsListIndex],[[['couponsList','',couponsListIndex,'id']]]]]]]}}" class="bnt bg-color-red" bindtap="__e">立即领取</view></block></block></view></view></view></block></view></block><loading vue-id="3" loaded="{{loadend}}" loading="{{loading}}" bind:__l="__l"></loading><block wx:if="{{couponsList.length===0&&page>1}}"><view class="noCommodity"><view class="noPictrue"><image class="image" src="{{$VUE_APP_RESOURCES_URL+'/images/noCoupon.png'}}"></image></view></view></block></view>