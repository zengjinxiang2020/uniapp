<view class="user"><view class="header bg-color-red acea-row row-between-wrapper"><view class="picTxt acea-row row-between-wrapper"><view class="pictrue"><image src="{{userInfo.avatar}}"></image></view><view class="text"><view class="acea-row row-middle"><view class="name line1">{{userInfo.nickname}}</view><block wx:if="{{userInfo.vip}}"><view class="member acea-row row-middle"><image src="{{userInfo.vipIcon}}"></image><text>{{userInfo.vipName}}</text></view></block></view><block wx:if="{{userInfo.phone}}"><view data-event-opts="{{[['tap',[['goPersonalData']]]]}}" class="id" bindtap="__e"><text>{{"ID："+(userInfo.uid||0)}}</text><text class="iconfont icon-bianji1"></text></view></block><block wx:else><button class="binding" open-type="getPhoneNumber" data-event-opts="{{[['getphonenumber',[['getPhoneNumber',['$event']]]]]}}" bindgetphonenumber="__e"><text>绑定手机号</text></button></block></view></view><text data-event-opts="{{[['tap',[['goPersonalData']]]]}}" class="iconfont icon-shezhi" bindtap="__e"></text></view><view class="wrapper"><view class="nav acea-row row-middle"><view data-event-opts="{{[['tap',[['goUserAccount']]]]}}" class="item" bindtap="__e"><text>我的余额</text><text class="num">{{userInfo.nowMoney||0}}</text></view><block wx:if="{{userInfo.isPromoter===1||userInfo.statu===2}}"><view data-event-opts="{{[['tap',[['goUserPromotion']]]]}}" class="item" bindtap="__e"><text>当前佣金</text><text class="num">{{userInfo.brokeragePrice||0}}</text></view></block><block wx:else><view data-event-opts="{{[['tap',[['goIntegral']]]]}}" class="item" bindtap="__e"><text>当前积分</text><text class="num">{{userInfo.integral||0}}</text></view></block><view data-event-opts="{{[['tap',[['goUserCoupon']]]]}}" class="item" bindtap="__e"><text>优惠券</text><text class="num">{{userInfo.couponCount||0}}</text></view></view><view class="myOrder"><view class="title acea-row row-between-wrapper"><text>我的订单</text><text data-event-opts="{{[['tap',[['goMyOrder']]]]}}" class="allOrder" bindtap="__e">全部订单<text class="iconfont icon-jiantou"></text></text></view><view class="orderState acea-row row-middle"><view data-event-opts="{{[['tap',[['goMyOrder',[0]]]]]}}" class="item" bindtap="__e"><view class="pictrue"><image src="{{$VUE_APP_RESOURCES_URL+'/images/dfk.png'}}"></image><block wx:if="{{orderStatusNum.unpaidCount>0}}"><text class="order-status-num">{{orderStatusNum.unpaidCount}}</text></block></view><view>待付款</view></view><view data-event-opts="{{[['tap',[['goMyOrder',[1]]]]]}}" class="item" bindtap="__e"><view class="pictrue"><image src="{{$VUE_APP_RESOURCES_URL+'/images/dfh.png'}}"></image><block wx:if="{{orderStatusNum.unshippedCount>0}}"><text class="order-status-num">{{orderStatusNum.unshippedCount}}</text></block></view><view>待发货</view></view><view data-event-opts="{{[['tap',[['goMyOrder',[2]]]]]}}" class="item" bindtap="__e"><view class="pictrue"><image src="{{$VUE_APP_RESOURCES_URL+'/images/dsh.png'}}"></image><block wx:if="{{orderStatusNum.receivedCount>0}}"><text class="order-status-num">{{orderStatusNum.receivedCount}}</text></block></view><text>待收货</text></view><view data-event-opts="{{[['tap',[['goMyOrder',[3]]]]]}}" class="item" bindtap="__e"><view class="pictrue"><image src="{{$VUE_APP_RESOURCES_URL+'/images/dpj.png'}}"></image><block wx:if="{{orderStatusNum.evaluatedCount>0}}"><text class="order-status-num">{{orderStatusNum.evaluatedCount}}</text></block></view><text>待评价</text></view><view data-event-opts="{{[['tap',[['goReturnList']]]]}}" class="item" bindtap="__e"><view class="pictrue"><image src="{{$VUE_APP_RESOURCES_URL+'/images/sh.png'}}"></image><block wx:if="{{orderStatusNum.refundCount>0}}"><text class="order-status-num">{{orderStatusNum.refundCount}}</text></block></view><text>售后/退款</text></view></view></view><view class="myService"><view class="title acea-row row-middle"><text>我的服务</text></view><view class="serviceList acea-row row-middle"><block wx:for="{{MyMenus}}" wx:for-item="item" wx:for-index="MyMenusIndex" wx:key="MyMenusIndex"><block wx:if="{{item.url}}"><view data-event-opts="{{[['tap',[['goPages',[MyMenusIndex]]]]]}}" class="item" bindtap="__e"><view class="pictrue"><image src="{{item.pic}}"></image></view><view>{{item.name}}</view></view></block></block></view></view></view><view style="text-align:center;margin-top:1rem;">By@意象</view><view class="footer-line-height"></view><switch-window vue-id="1" switchActive="{{switchActive}}" login_type="{{userInfo.login_type}}" data-event-opts="{{[['^changeswitch',[['changeswitch']]]]}}" bind:changeswitch="__e" bind:__l="__l"></switch-window></view>